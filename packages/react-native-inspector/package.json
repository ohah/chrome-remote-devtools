{
  "name": "@ohah/chrome-remote-devtools-react-native",
  "version": "0.1.0",
  "description": "Chrome Remote DevTools Inspector Plugin for React Native",
  "keywords": [
    "debugging",
    "devtools",
    "inspector",
    "react-native"
  ],
  "license": "MIT",
  "author": "Chrome Remote DevTools",
  "repository": {
    "type": "git",
    "url": "https://github.com/ohah/chrome-remote-devtools.git",
    "directory": "packages/react-native-inspector"
  },
  "files": [
    "README.md",
    "android/",
    "dist/",
    "ios/",
    "src/"
  ],
  "sideEffects": true,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "src/index.ts",
  "react-native": "src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "react-native": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./zustand": {
      "types": "./src/zustand.ts",
      "react-native": "./src/zustand.ts",
      "default": "./src/zustand.ts"
    },
    "./zustand/middleware": {
      "types": "./src/zustand/middleware.ts",
      "react-native": "./src/zustand/middleware.ts",
      "default": "./src/zustand/middleware.ts"
    },
    "./metro": {
      "react-native": "./src/metro.js",
      "require": "./src/metro.js",
      "import": "./src/metro.js",
      "default": "./src/metro.js"
    },
    "./redux-devtools-extension": {
      "types": "./src/redux-devtools-extension.ts",
      "react-native": "./src/redux-devtools-extension.ts",
      "require": "./src/redux-devtools-extension.ts",
      "import": "./src/redux-devtools-extension.ts",
      "default": "./src/redux-devtools-extension.ts"
    }
  },
  "scripts": {
    "build": "tsdown",
    "build:clean": "rm -rf dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@babel/runtime": "^7.25.0"
  },
  "devDependencies": {
    "@types/react": "^19.2.7",
    "tsdown": "^0.16.8",
    "typescript": "^5.9.3",
    "zustand": "^5.0.9"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-native": ">=0.83.0",
    "zustand": "^5.0.9"
  },
  "peerDependenciesMeta": {
    "zustand": {
      "optional": true
    }
  }
}
